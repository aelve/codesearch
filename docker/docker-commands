-- запуск базы --
docker run -d -p 5432:5432 -v /Users/yan/Desktop/postshaer:/var/lib/postgresql -e POSTGRES_DB='sourcesdb' \ 
-e POSTGRES_USER='postgres' \
-e POSTGRES_PASSWORD='postgres' \
--name postgres postgres

-- запуск core
docker run -d -v /Users/yan/Desktop/testData:/data -v /Users/yan/Desktop/testDataNpm/:/index/npm --link postgres:postgres --name core core make download-haskell 

-- запуск server
docker run -p 9000:80 -d -v /Users/yan/Desktop/testData:/data --link postgres:postgres --name web server
